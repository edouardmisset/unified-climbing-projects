{
  "name": "climbing-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "format": "biome format --write ./src",
    "lint": "biome lint --write ./src",
    "check": "biome check --fix ./src && bun run style:lint && tsc --noEmit && ls-lint",
    "commitlint": "commitlint --edit",
    "dev": "next dev --turbo",
    "preview": "bun run build && next start",
    "reset": "rm -rf node_modules bun.lock* && bun install --save-text-lockfile",
    "spell": "cspell lint --no-progress --show-suggestions --show-context --locale 'en,fr' ./src/**/*.ts*",
    "start": "next start",
    "style:lint": "stylelint --fix --cache 'src/**/*.css'",
    "test:unit": "bun poku --reporter=dot ./src",
    "test:unit:watch": "bun run test --watch",
    "test:e2e": "bun run playwright test",
    "test:e2e:ui": "bun run test:e2e --ui",
    "test:all": "bun run test:unit && bun run test:e2e",
    "knip": "clear && knip",
    "ls-lint": "ls-lint",
    "validate": "clear && bun run check && bun run test:all && bun run build",
    "client:rsdoctor": "rsdoctor analyze --profile .next/.rsdoctor/manifest.json",
    "server:rsdoctor": "rsdoctor analyze --profile .next/server/chunks/.rsdoctor/manifest.json"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.1",
    "@clerk/nextjs": "^6.23.3",
    "@clerk/themes": "^2.2.54",
    "@edouardmisset/array": "npm:@jsr/edouardmisset__array@^4.0.3",
    "@edouardmisset/date": "npm:@jsr/edouardmisset__date@^4.2.0",
    "@edouardmisset/math": "npm:@jsr/edouardmisset__math@^4.0.0",
    "@edouardmisset/object": "npm:@jsr/edouardmisset__object@^4.0.1",
    "@edouardmisset/text": "npm:@jsr/edouardmisset__text@^4.1.0",
    "@nivo/bar": "^0.88.0",
    "@nivo/core": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@nivo/pie": "^0.88.0",
    "@std/csv": "npm:@jsr/std__csv",
    "@std/text": "npm:@jsr/std__text@^1.0.15",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@trpc/client": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "fuzzysort": "^3.1.0",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4",
    "humanize-duration": "^3.33.0",
    "lucide-react": "^0.525.0",
    "next": "^15.4.4",
    "next-view-transitions": "^0.3.4",
    "nuqs": "^2.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-toastify": "^11.0.5",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@ls-lint/ls-lint": "^2.3.1",
    "@playwright/test": "^1.53.2",
    "@rsdoctor/cli": "^1.1.7",
    "@rsdoctor/webpack-plugin": "^1.1.7",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/humanize-duration": "^3.27.4",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "cspell": "^9.1.3",
    "knip": "^5.61.3",
    "poku": "^3.0.2",
    "react-scan": "^0.4.3",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "^5.8.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@clerk/shared",
    "@vercel/speed-insights",
    "core-js",
    "esbuild",
    "oxc-resolver",
    "sharp"
  ]
}
