{
  "editor.codeActionsOnSave": {
    "source.addMissingImports.ts": "explicit"
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "oxc.oxc-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": "explicit"
    }
  },
  "typescript.experimental.useTsgo": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#af16f5",
    "activityBar.background": "#af16f5",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#f4ab0b",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#af16f5",
    "statusBar.background": "#9109cf",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#af16f5",
    "statusBarItem.remoteBackground": "#9109cf",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#9109cf",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#9109cf99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#9109cf",
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": ["BOULDERING", "neobrutalism"]
}
